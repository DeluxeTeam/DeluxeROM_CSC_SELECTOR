<favorites xmlns:launcher="http://schemas.android.com/apk/res/com.sec.android.app.launcher">

    <home>
        <!--  screen [0]  -->
        <!-- appwidget : Weather -->
        <appwidget
            launcher:screen="0" 
            launcher:packageName="com.sec.android.daemonapp" 
            launcher:className="com.sec.android.daemonapp.appwidget.WeatherAppWidget2x1" 
            launcher:x="1" 
            launcher:y="0" 
            launcher:spanX="3" 
            launcher:spanY="3" />

        <!-- appwidget : Google app -->
        <appwidget
            launcher:screen="0" 
            launcher:packageName="com.google.android.googlequicksearchbox" 
            launcher:className="com.google.android.googlequicksearchbox.SearchWidgetProvider" 
            launcher:x="0" 
            launcher:y="4" 
            launcher:spanX="5" 
            launcher:spanY="1" />
            
        <!--  screen [1]  -->
        <!--ClaroJuegos-->
        <appwidget
            launcher:packageName="com.gameloft.android.gdc"
            launcher:className="com.gameloft.android.gdc.GDCWidgetProvider"
            launcher:screen="1"
            launcher:x="0"
            launcher:y="0"
            launcher:spanX="4"
            launcher:spanY="4"
            launcher:postPosition="true" />      

        <!-- folder : Servicios -->
        <folder 
            launcher:screen="1" 
            launcher:title="Servicios" 
            launcher:x="0" 
            launcher:y="4">
			<!-- Claro m?ica MX -->
			<favorite
				launcher:screen="0"
				launcher:packageName="com.telcel.imk"
				launcher:className="com.telcel.imk.MainActivity" />

			<!-- ClaroVideo -->
			<favorite
				launcher:screen="0"
				launcher:packageName="com.dla.android"
				launcher:className="com.clarovideo.app.ui.activities.SplashActivity" />

			<!-- Mi Telcel -->
			<favorite
				launcher:screen="0"
				launcher:packageName="com.speedymovil.wire"
				launcher:className="com.speedymovil.wire.Login" />

			<!-- Transfer 3.0.0.3-->
			<favorite
				launcher:screen="0"
				launcher:packageName="com.citi.transfer"
				launcher:className="com.citi.transfer.ui.activities.SplashActivity" />

        </folder>

        <folder
            launcher:screen="1"
            launcher:title="Juegos"
            launcher:x="1"
            launcher:y="4">
            <!-- Claro Juegos -->
            <favorite
                launcher:packageName="com.gameloft.android.gdc" 
                launcher:className="com.gameloft.android.gdc.AYCELoadingActivity"
                launcher:screen="0"
                launcher:postPosition="true"/>

            <!-- 5 Gameloft -->
            <!-- Asphalt Nitro --> 
            <favorite
                launcher:screen="1" 
                launcher:packageName="com.gameloft.android.GloftANPH" 
                launcher:className="com.gameloft.android.GloftANPH.Start"
                launcher:postPosition="true"/> 

            <!-- Kingdoms --> 
            <favorite
                launcher:screen="2" 
                launcher:packageName="com.gameloft.android.GloftDMKF" 
                launcher:className="com.gameloft.android.GloftDMKF.Start"
                launcher:postPosition="true"/> 

            <!-- Little Big City 2 --> 
            <favorite
                launcher:screen="3"
                launcher:packageName="com.gameloft.android.GloftMBCF" 
                launcher:className="com.gameloft.android.GloftMBCF.Start"
                launcher:postPosition="true"/> 

            <!--  Scrat-ventures --> 
            <favorite
                launcher:screen="4" 
                launcher:packageName="com.gameloft.android.GloftSCRT" 
                launcher:className="com.gameloft.android.GloftSCRT.Start"
                launcher:postPosition="true"/> 

            <!-- Spider-Man: Ultimate Power -->
            <favorite
                launcher:screen="5" 
                launcher:packageName="com.gameloft.android.GloftSMIF" 
                launcher:className="com.gameloft.android.GloftSMIF.Start"
                launcher:postPosition="true"/>
            <!--  Dragon Mania --> 
            <favorite
                launcher:screen="6" 
                launcher:packageName="com.gameloft.android.GloftPDMF" 
                launcher:className="com.gameloft.android.GloftPDMF.Start"
                launcher:postPosition="true"/>
          
        </folder>

      	<!-- Facebook -->
        <favorite
            launcher:screen="1"
            launcher:x="2"
            launcher:y="4"
            launcher:packageName="com.facebook.katana" 
            launcher:className="com.facebook.katana.LoginActivity"  />

    </home> 

    <hotseat>
        <!-- Contacts -->
        <favorite
            launcher:screen="0" 
            launcher:packageName="com.samsung.android.contacts" 
            launcher:className="com.android.dialer.DialtactsActivity" />

      	<!-- Contenedor -->
        <favorite
            launcher:screen="1" 
            launcher:packageName="com.telcel.contenedor"
            launcher:className="com.claroColombia.contenedor.gui.activities.AppsContainerActivity" />

        <!-- Messages -->
        <favorite
            launcher:screen="2" 
            launcher:packageName="com.samsung.android.messaging" 
            launcher:className="com.android.mms.ui.ConversationComposer" />

        <!-- Internet -->
        <favorite
            launcher:screen="3" 
            launcher:packageName="com.sec.android.app.sbrowser" 
            launcher:className="com.sec.android.app.sbrowser.SBrowserMainActivity" />

        <!-- Google Play Store -->
        <favorite
            launcher:screen="4" 
            launcher:packageName="com.android.vending" 
            launcher:className="com.android.vending.AssetBrowserActivity" />

    </hotseat> 

</favorites>